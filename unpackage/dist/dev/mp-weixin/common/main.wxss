}
/**
 * 文字超出部分使用省略号
 */
.single-text {
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    max-width: 100%;
}
.multi-text {
    max-width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    min-height: 76rpx;
    line-height: 38rpx;
    font-size: 28rpx;
    white-space: initial;
}
