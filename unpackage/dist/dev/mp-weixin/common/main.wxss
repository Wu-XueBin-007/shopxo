    background: #f5f5f5;
    color: #4a4a4a;
}
input[type="text"],
input[type="number"],
input[type="idcard"],
input[type="digit"],
textarea {
    -webkit-appearance: none;
    border-radius: 5px;
    box-sizing: border-box;
}
page,
textarea,
.a-textarea-control textarea {
    font-size: 28rpx;
}
button:after,
button:before {
    border: 0;
    border-radius: 0;
}
