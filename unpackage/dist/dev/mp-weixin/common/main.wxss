/**
 * 分享组建样式
 */
.share-popup {
    padding: 20rpx 10rpx 0 10rpx;
    position: relative;
}
.share-popup .close {
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    z-index: 2;
}
